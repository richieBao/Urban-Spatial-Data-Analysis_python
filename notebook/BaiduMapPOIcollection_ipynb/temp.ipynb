   "source": [
    "import sympy\n",
    "from sympy import *\n",
    "init_printing(use_unicode=True)\n",
    "\n",
    "x,y,z=symbols('x y z')\n",
    "a,b=symbols('a b',real=True)\n",
    "k,m,n=symbols('k m n')\n",
    "\n",
    "expr=x**2+x*y\n",
    "srepr(expr)"
   ]
